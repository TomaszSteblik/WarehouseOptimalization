<Window x:Class="OptimizationUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:InteractiveDataDisplay.WPF;assembly=InteractiveDataDisplay.WPF"
        xmlns:views="clr-namespace:OptimizationUI.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="860" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Distance">
                <views:DistanceView DataContext="{Binding DistanceViewModel}"/>
            </TabItem>
            <TabItem Header="Warehouse">
                <views:WarehouseView DataContext="{Binding WarehouseViewModel}"/>
            </TabItem>
            <TabItem Header="Keyboard">
                <StackPanel Background="#FFE5E5E5">
                    
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
